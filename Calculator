
import java.util.Scanner;
public class Aryan {

public static void main(String[]args){

Scanner input=new Scanner(System.in);

//in your input, put the first number, then whatever operation, then the second number

long x=input.nextLong();
long y=input.nextLong();
String z=input.next();
switch (z) {
    case "+":
        System.out.println(x+y);
        break;
    case "*":
    System.out.println(x*y);
        break;
    case "-":
        System.out.println(x-y);
        break;
    case "/":
        System.out.println(x/y);
        break;
}

}
}

