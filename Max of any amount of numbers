import java.util.Scanner;
public class Aryan {
    public static void main(String args[]) {
        Scanner input=new Scanner(System.in);
        int n = input.nextInt();
        //n is the number of numbers in the input it has to scan
        int a = input.nextInt();
        int i;
        for (i=0; i<n-1; i++) {
            int x = input.nextInt();
            if (x>a) {
                a = x;
            }
            
        }
        System.out.print(a);
    }}
