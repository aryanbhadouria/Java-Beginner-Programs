import java.util.Scanner;
public class JavaClass{

public static void main(String[]args){

Scanner input=new Scanner(System.in);
int n = input.nextInt();
//n is the number of rows
String y=input.next();
//y is what you want the triangle made out of
for (int i=0; i<n; i++){
    for (int a=0;a<1+i;a++){
        System.out.print(y +" ");
            
        
        
    }
    System.out.print("\n");
}
}
}


