import java.util.Scanner;
public class Aryan{

public static void main(String[]args){
//a is column, i is row
Scanner input=new Scanner(System.in);
int n = input.nextInt();
for (int i=0; i<n; i++){
    for (int a=0;a<n;a++){
        System.out.print((i*n+1)+(a) + " ");
            
        
        
    }
    System.out.print("\n");
}
}
}
